{
  "name": "bot",
  "version": "1.0.0",
  "description": "bot web",
  "main": "index.js",
  "engines": {
    "node": "12.14.1",
    "npm": "6.13.4"
  },
  "scripts": {
    "start": "node index.js",
    "backend": "nodemon index.js",
    "frontend": "npm run front --prefix client",
    "dev": "concurrently \"npm run backend\"  \"npm run start --prefix client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Pedro Perez",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.1",
    "dialogflow": "^1.1.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "nodemon": "^2.0.2"
  }
}
